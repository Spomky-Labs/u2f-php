{
    "name": "spomky-labs/u2f-php",
    "type": "library",
    "license": "MIT",
    "description": "FIDO/FIDO2 U2F Support For PHP",
    "keywords": ["FIDO", "U2F"],
    "homepage": "https://github.com/Spomky-Labs/u2f-php",
    "authors": [
        {
            "name": "Florent Morselli",
            "homepage": "https://github.com/Spomky"
        },
        {
            "name": "All contributors",
            "homepage": "https://github.com/Spomky-Labs/u2f-php/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "ext-openssl": "*",
        "spomky-labs/base64url": "^2.0",
        "spomky-labs/cbor-php": "^1.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "U2FAuthentication\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "U2FAuthentication\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
